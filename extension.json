{
	"name": "Shibboleth",
	"version": "1.0",
	"author": [
		"[https://niif.hu northway]"
	],
	"url": "https://www.github.com",
	"descriptionmsg": "shibboleth-desc",
	"license-name": "MIT",
	"type": "other",
	"requires": {
		"extensions": {
			"PluggableAuth": ">= 5.1"
		}
	},
	"MessagesDirs": {
		"Shibboleth": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"Shibboleth": "Shibboleth.class.php"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\Shibboleth\\": "src/",
		"MediaWiki\\Extension\\Shibboleth\\Tests\\": "tests/phpunit/"
	},
	"Hooks": {
		"PluggableAuthPopulateGroups": ["Shibboleth::populateGroups"]
	},
	"config": {
		"PluggableAuth_Class": "Shibboleth",
		"Shibboleth_UserProviderFactoryCallback": "MediaWiki\\Extension\\Shibboleth\\UserInfoProvider\\ServerSuperGlobal::factory",
		"Shibboleth_GroupMap": null,
		"ShibDisplayName": "displayName",
		"ShibEmail": "mail",
		"ShibUsername": "eppn"
	},
	"manifest_version": 1
}
